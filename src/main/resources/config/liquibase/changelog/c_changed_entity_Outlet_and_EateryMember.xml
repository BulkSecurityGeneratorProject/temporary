<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">


    <changeSet id="create_outlet_eatery_member_table" author="rostyslav_dyyak" dbms="postgresql">
        <createTable tableName="T_EATERY_OUTLET_MEMBER">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="eatery_member_outlet" type="bigint" />
            <column name="outlet_id" type="bigint" />
        </createTable>
        <addForeignKeyConstraint baseColumnNames="eatery_member_outlet"
                                 baseTableName="T_EATERY_OUTLET_MEMBER"
                                 constraintName="fk_eatery_memeber_id"
                                 referencedColumnNames="id"
                                 referencedTableName="T_EATERY_MEMBER"/>
        <addForeignKeyConstraint baseColumnNames="outlet_id"
                                 baseTableName="T_EATERY_OUTLET_MEMBER"
                                 constraintName="fk_outlet_eatery_memeber_id"
                                 referencedColumnNames="id"
                                 referencedTableName="T_OUTLET"/>
    </changeSet>
</databaseChangeLog>
